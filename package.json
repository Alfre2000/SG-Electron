{
  "name": "SuperGalvanica",
  "description": "Applicazione per la gestione dei processi aziendali",
  "author": "Alfredo Mauri",
  "version": "2.5.2",
  "private": false,
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.7",
    "electron-updater": "^5.0.1",
    "electron-window-state": "^5.0.3",
    "framer-motion": "^6.3.16",
    "jszip": "^3.10.1",
    "react": "^18.0.0",
    "react-bootstrap": "^2.3.0",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.3.2",
    "tedious": "^15.1.3",
    "web-vitals": "^2.1.4"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "wait-on tcp:3000 && electron .",
    "watch": "electron .",
    "dev": "BROWSER=none npm start",
    "build-el": "npm run build && electron-builder -mw",
    "build-m": "npm run build && electron-builder -m",
    "release": "npm run build && electron-builder -mw -p 'onTagOrDraft'"
  },
  "repository": "https://github.com/Alfre2000/SG-Electron",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3",
    "jest-watch-typeahead": "^0.6.5",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24"
  },
  "build": {
    "productName": "SuperGalvanica",
    "appId": "it.SuperGalvanica",
    "publish": {
      "provider": "github"
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.business",
      "target": "default"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "build/**/*"
    ]
  }
}
