    "jest-watch-typeahead": "^0.6.5",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.9.5"
  },
  "build": {
    "productName": "SuperGalvanica",
    "appId": "it.SuperGalvanica",
    "publish": {
      "provider": "github"
    },
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.business",
      "target": "default"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "build/**/*"
    ]
  }
