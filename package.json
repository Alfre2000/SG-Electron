  "main": "public/electron.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "electron": "wait-on tcp:3000 && electron .",
    "watch": "electron .",
    "dev": "BROWSER=none npm start",
    "build-el": "npm run build && electron-builder -mw",
    "build-m": "npm run build && electron-builder -m",
    "release": "npm run build && electron-builder -mw -p 'onTagOrDraft'"
  },
  "repository": "https://github.com/Alfre2000/SG-Electron",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@craco/craco": "^7.1.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
